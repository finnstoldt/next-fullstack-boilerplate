{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "db:generate": "yarn prisma generate",
    "db:migrate": "yarn prisma migrate deploy",
    "db:seed": "yarn prisma db seed",
    "start": "yarn db:migrate && yarn db:generate && yarn prisma db seed && node -r esbuild-register ./src/index.ts",
    "build": "yarn db:generate && tsc",
    "lint": "tsc --noEmit && cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.15",
    "scripts": "*",
    "supertest": "^6.1.3",
    "tsconfig": "*"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.11",
    "@graphql-tools/load": "^7.5.10",
    "@prisma/client": "^3.12.0",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "graphql": "^16.4.0",
    "graphql-codegen": "*",
    "logger": "*",
    "morgan": "^1.10.0",
    "prisma": "^3.12.0",
    "typescript": "^4.5.3",
    "esbuild-register": "^2.5.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
